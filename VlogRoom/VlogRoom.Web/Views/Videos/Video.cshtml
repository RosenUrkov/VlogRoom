                        <li><a href="#" class="icon comment-icon">Comments</a></li>
                        <li class="view">200 Views</li>
                    </ul>
                </div>
            </div>
            <div class="clearfix"> </div>
            <div class="published">
                <div class="load_more">
                    <ul id="myList">
                        <li>
                            <h4>Published on @Model.CreatedOn.Value</h4>
                            <p>@Model.Description</p>
                        </li>                        
                    </ul>
                </div>
            </div>
