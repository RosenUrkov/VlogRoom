                <div class="song-info">
                    <h3>@Model.Title</h3>
                </div>
                <div class="video-grid">
                    <iframe width="854" height="480" src="@("https://www.youtube.com/embed/" + Model.ServiceVideoId)" frameborder="0" allowfullscreen></iframe>
                </div>
            </div>
            <div class="song-grid-right">
